{
  "name": "atom-bugs",
  "version": "1.5.7",
  "author": "Williams Medina <williams.medinaa@gmail.com>",
  "description": "A Simple Debugger for Atom",
  "keywords": [
    "javascript",
    "debugger",
    "debug",
    "atom",
    "repl",
    "bugs",
    "chrome",
    "browser",
    "willyelm",
    "node",
    "breakpoint",
    "console"
  ],
  "license": "MIT",
  "repository": "https://github.com/atom-bugs/atom-bugs",
  "main": "./lib/main",
  "showWelcome": true,
  "scripts": {
    "start": "tsc -w"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "atom-bugs": {
      "versions": {
        "1.0.0": "provideService"
      }
    }
  },
  "configSchema": {
    "toolbarStyle": {
      "title": "Toolbar Style",
      "description": "Change toolbar panel position.",
      "type": "string",
      "default": "TopPanel",
      "enum": [
        {
          "value": "TopPanel",
          "description": "Top"
        },
        {
          "value": "BottomPanel",
          "description": "Bottom"
        },
        {
          "value": "HeaderPanel",
          "description": "Title"
        }
      ]
    },
    "showToolbarIcon": {
      "title": "Atom Bugs Icon",
      "description": "Show atom-bugs icon on toolbar.",
      "type": "boolean",
      "default": true
    }
  },
  "package-deps": [
    "atom-bugs-nodejs"
  ],
  "dependencies": {
    "atom-package-deps": "^4.6.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/atom": "0.0.36",
    "@types/lodash": "^4.14.62",
    "@types/node": "^7.0.8",
    "typescript": "^2.3.0"
  },
  "atom-bugs": [
    {
      "scheme": "Develop",
      "plugin": "Node.js",
      "settings": {
        "binaryPath": "",
        "portNumber": "5858",
        "lauchArguments": [],
        "environmentVariables": {}
      }
    }
  ]
}
