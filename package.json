{
  "name": "atom-bugs",
  "version": "1.2.6",
  "author": "Williams Medina <williams.medinaa@gmail.com>",
  "description": "A Simple Debugger for Atom",
  "keywords": [
    "javascript",
    "debugger",
    "debug",
    "atom",
    "repl",
    "bugs",
    "chrome",
    "browser",
    "willyelm",
    "node",
    "breakpoint",
    "console"
  ],
  "license": "MIT",
  "repository": "https://github.com/atom-bugs/atom-bugs",
  "main": "./lib/main",
  "showWelcome": true,
  "scripts": {
    "start": "tsc -w"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "atom-bugs": {
      "versions": {
        "1.0.0": "provideService"
      }
    }
  },
  "configSchema": {
    "showToolbarIcon": {
      "title": "Icon",
      "description": "Show atom-bugs icon on toolbar.",
      "type": "boolean",
      "default": true
    }
  },
  "package-deps": [
    "atom-bugs-nodejs"
  ],
  "dependencies": {
    "atom-package-deps": "^4.4.1"
  },
  "devDependencies": {
    "@types/atom": "0.0.36",
    "@types/node": "^7.0.8",
    "typescript": "^2.2.1"
  }
}
